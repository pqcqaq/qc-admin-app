{
  "globalStyle": {
    "navigationStyle": "default",
    "navigationBarTitleText": "patrol",
    "navigationBarBackgroundColor": "#f8f8f8",
    "navigationBarTextStyle": "black",
    "backgroundColor": "#FFFFFF"
  },
  "easycom": {
    "autoscan": true,
    "custom": {
      "^fg-(.*)": "@/components/fg-$1/fg-$1.vue",
      "^wd-(.*)": "wot-design-uni/components/wd-$1/wd-$1.vue",
      "^(?!z-paging-refresh|z-paging-load-more)z-paging(.*)": "z-paging/components/z-paging$1/z-paging$1.vue"
    }
  },
  "tabBar": {
    "color": "#999999",
    "selectedColor": "#018d71",
    "backgroundColor": "#F8F8F8",
    "borderStyle": "black",
    "height": "50px",
    "fontSize": "10px",
    "iconWidth": "24px",
    "spacing": "3px",
    "custom": true,
    "list": [
      {
        "iconPath": "/static/tabbar/home.png",
        "selectedIconPath": "/static/tabbar/homeHL.png",
        "pagePath": "pages/index/index",
        "text": ""
      },
      {
        "iconPath": "/static/tabbar/History.png",
        "selectedIconPath": "/static/tabbar/HistoryHL.png",
        "pagePath": "pages/task/index",
        "text": ""
      },
      {
        "iconPath": "/static/tabbar/Category.png",
        "selectedIconPath": "/static/tabbar/CategoryHL.png",
        "pagePath": "pages/mine/index",
        "text": ""
      }
    ]
  },
  "pages": [
    {
      "path": "pages/index/index",
      "type": "home",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "首页"
      },
      "layout": "tabbar"
    },
    {
      "path": "pages/about/about",
      "type": "page",
      "layout": "tabbar"
    },
    {
      "path": "pages/about/i18n",
      "type": "page",
      "style": {
        "navigationBarTitleText": "%app.name%"
      }
    },
    {
      "path": "pages/login/index",
      "type": "page",
      "style": {
        "navigationBarTitleText": "登录",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/login/privacy",
      "type": "page"
    },
    {
      "path": "pages/login/service",
      "type": "page"
    },
    {
      "path": "pages/mine/index",
      "type": "page",
      "style": {
        "navigationBarTitleText": "我的"
      },
      "layout": "tabbar"
    },
    {
      "path": "pages/task/index",
      "type": "page",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "任务看板"
      },
      "layout": "tabbar"
    },
    {
      "path": "pages/mine/about/index",
      "type": "page",
      "style": {
        "navigationBarTitleText": "关于我们"
      }
    },
    {
      "path": "pages/mine/info/index",
      "type": "page",
      "style": {
        "navigationBarTitleText": "个人资料"
      }
    },
    {
      "path": "pages/mine/password/index",
      "type": "page",
      "style": {
        "navigationBarTitleText": "修改密码"
      }
    },
    {
      "path": "pages/task/history/index",
      "type": "page",
      "style": {
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/mine/info/phone/index",
      "type": "page",
      "style": {
        "navigationBarTitleText": "修改手机号",
        "navigationBarLeftText": "取消",
        "navigationBarTitleAlign": "center",
        "navigationBarRightButtonList": [
          {
            "text": "完成",
            "color": "#007aff",
            "fontSize": "16"
          }
        ]
      }
    },
    {
      "path": "pages/mine/info/username/index",
      "type": "page",
      "style": {
        "navigationStyle": "custom"
      }
    }
  ],
  "subPackages": []
}